        <item name="colorPrimary">#00205b</item>
        <item name="colorPrimaryVariant">#00205b</item>
        <item name="colorOnPrimary">#000</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

